<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="BizCollege.DataAccessLayer"
                   namespace="BizCollege.DataAccessLayer.Domain"
                   default-lazy="false">
  <class name="Enrollment" lazy="false">
    <id name="Id">
      <generator class="uuid.hex"/>
    </id>
    <property name="CourseId"/>
    <property name="DateStarted"/>
    <property name="WasCourseCompleted"/>
    <property name="DateCompleted"/>
    <property name="IdOfLastCourseSlideAccessed"/>
    
  </class>
</hibernate-mapping>